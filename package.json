{
  "name": "ai-tutor",
  "version": "1.0.0",
  "description": "AI-powered tutoring platform",
  "main": "dist/server.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon src/server.ts",
    "server:build": "tsc",
    "server:start": "node dist/server.js",
    "client:dev": "cd client && npm start",
    "client:build": "cd client && npm run build",
    "install:all": "npm install && cd client && npm install"
  },
  "keywords": ["ai", "tutor", "education", "chatbot"],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "socket.io": "^4.7.4",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.20.1",
    "express-validator": "^7.0.1",
    "form-data": "^4.0.0",
    "node-fetch": "^3.3.2",
    "ws": "^8.14.2",
    "fluent-ffmpeg": "^2.1.2",
    "sharp": "^0.32.6",
    "jimp": "^0.22.10"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "@types/fluent-ffmpeg": "^2.1.24",
    "typescript": "^5.3.2",
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2",
    "ts-node": "^10.9.1"
  }
} 